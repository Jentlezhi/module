#import "CYTMyAccountCtr.h"
#import "CYTWithdrawTableController.h"

@implementation CYTMyAccountCtr

- (void)viewDidLoad {
    [super viewDidLoad];
    [self createNavBarWithTitle:@"我的账户" andShowBackButton:YES showRightButtonWithTitle:@"提现"];
    @weakify(self);
    [[[NSNotificationCenter defaultCenter] rac_addObserverForName:kRefreshMyAccount object:nil] subscribeNext:^(id x) {
        @strongify(self);
        [self.webView reload];
    }];
}

- (void)rightButtonClick:(UIButton *)rightButton {
    CYTWithdrawTableController *withdraw = [CYTWithdrawTableController new];
    withdraw.viewModel.balanceWithdraw = YES;
    [self.navigationController pushViewController:withdraw animated:YES];
}

@end
