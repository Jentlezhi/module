#import <Foundation/Foundation.h>

@interface CYTSettingItemModel : NSObject

/** 标题 */
@property(copy, nonatomic) NSString *title;
/** 内容 */
@property(copy, nonatomic) NSString *content;
/** 显示指示 */
@property(assign, nonatomic) BOOL showIndicator;
/** 显示图片 */
@property(assign, nonatomic) BOOL showHeader;
/** 是否仅有标题 */
@property (nonatomic, assign) BOOL onlyTitle;
/** 图片url */
@property(copy, nonatomic) NSString *picUrl;
/** 是否隐藏分割线 */
@property(assign, nonatomic) BOOL hiddeDividerLine;


@end
