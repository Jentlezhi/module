#import "CYTShakeContentView.h"

@interface CYTShakeContentView ()
@property (nonatomic, strong) UIImageView *imageView;

@end

@implementation CYTShakeContentView

- (void)ff_addSubViewAndConstraints {
    [super ff_addSubViewAndConstraints];
    
    [self addSubview:self.imageView];
    [self.imageView makeConstraints:^(MASConstraintMaker *make) {
        make.edges.equalTo(0);
        
    }];
}

#pragma mark- get
- (UIImageView *)imageView {
    if (!_imageView) {
        _imageView = [UIImageView imageViewWithImgName:nil];
//        [_imageView sd_animatedGIFNamed]
    }
    return _imageView;
}

@end
