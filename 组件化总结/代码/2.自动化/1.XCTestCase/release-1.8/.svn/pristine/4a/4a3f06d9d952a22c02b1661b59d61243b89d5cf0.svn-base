#import <Foundation/Foundation.h>

@interface CYTConfirmSendCarModel : NSObject

/** 标题 */
@property(copy, nonatomic) NSString *title;
/** 内容 */
@property(copy, nonatomic) NSString *content;
/** 是否显示箭头 */
@property(assign, nonatomic,readonly) BOOL showArrow;
/** 是否显示输入框 */
@property(assign, nonatomic,readonly) BOOL showTextField;
/** 是否显示分割线 */
@property(assign, nonatomic) BOOL showDividerLine;
/** 键盘类型 */
@property(assign, nonatomic) UIKeyboardType keyboardType;
/** 占位文字 */
@property(copy, nonatomic) NSString *placeHolder;
/** 输入值 */
@property(copy, nonatomic) NSString *inputContent;
/** 输入字数限制 */
@property(assign, nonatomic) NSInteger maxInputWord;

@end
